{
    "presets": ["es2017", "es2015", "stage-0", "react"],
    "plugins": ["transform-runtime", "transform-decorators-legacy", "transform-class-properties"],
    "env": {
        "commonjs": {
            "plugins": [
                ["transform-es2015-modules-commonjs", {
                    "loose": true
                }, "react-hot-loader/babel"]
            ]
        }
    }
}
