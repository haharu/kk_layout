{
  "name": "workbox",
  "scripts": {
    "deploy": "cross-env NODE_ENV=production webpack -p --config webpack-production.config.js --progress --colors",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register",
    "lint": "eslint test src",
    "css:build": "compass compile",
    "css:watch": "compass watch",
    "dev-webserver": "cross-env NODE_ENV=development node --harmony server.js",
    "dev-apiserver": "cross-env NODE_ENV=development node --harmony apiserver.js",
    "prod-webserver": "cross-env NODE_ENV=production node --harmony server.js",
    "prod-apiserver": "cross-env NODE_ENV=production node --harmony apiserver.js",
    "dev": "concurrently --kill-others \"npm run dev-webserver\" \"npm run dev-apiserver\"",
    "start": "concurrently --kill-others \"npm run prod-webserver\" \"npm run prod-apiserver\"",
    "postinstall": "npm run deploy"
  },
  "author": "haharu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "koa-webpack-dev-middleware": "^1.4.4",
    "koa-webpack-hot-middleware": "^1.0.3",
    "mocha": "^3.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "style-loader": "^0.14.0",
