    "koa-static": "^2.0.0",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-d3": "^0.4.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sass-mq": "^3.3.1"
  }
