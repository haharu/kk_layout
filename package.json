{
  "name": "workbox",
  "scripts": {
    "devserver": "cross-env NODE_ENV=development webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors --harmony",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack-dev-server.config.js --harmony --progress --colors",
    "deploy": "cross-env NODE_ENV=production webpack -p --config webpack-production.config.js --harmony --progress --colors",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register",
    "lint": "eslint test src",
    "css:build": "compass compile",
    "css:watch": "compass watch",
    "start": "cross-env NODE_ENV=production babel-node server.js",
    "postinstall": "npm run deploy"
  },
  "author": "haharu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "style-loader": "^0.13.1",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2"
  },
