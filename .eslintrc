{
    "extends": ["eslint:recommended", "google", "plugin:react/recommended"],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "new-cap": 1,
        "no-console": 1,
        "max-len": 0,
        "require-jsdoc": 0,
        "no-undef": 0,
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    }
}
